{
    "name": "comments",
    "description": "Blank Qwik starter app. Vite.js tooling. Express.js server. Eslint code linting. Prettier code formatter.",
    "scripts": {
        "build": "npm run typecheck && npm run build.client && npm run build.ssr",
        "build.client": "vite build",
        "build.ssr": "vite build --ssr src/entry.express.tsx",
        "dev": "vite",
        "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js",
        "dev.ssr": "vite --mode ssr",
        "fmt": "prettier --write .",
        "fmt.check": "prettier --check .",
        "lint": "eslint \"**/*.ts*\"",
        "serve": "node server/entry.express.js",
        "start": "npm run serve",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@types/eslint": "8.4.2",
        "@types/express": "4.17.13",
        "@types/node": "17.0.34",
        "@types/node-fetch": "^2.6.1",
        "@typescript-eslint/eslint-plugin": "5.25.0",
        "@typescript-eslint/parser": "5.25.0",
        "eslint": "8.15.0",
        "eslint-plugin-qwik": "latest",
        "prettier": "2.6.2",
        "typescript": "4.6.4",
        "vite": "2.9.9"
    },
    "engines": {
        "node": "14.17.x"
    },
    "homepage": "https://qwik.builder.io/",
    "license": "",
    "private": true,
    "dependencies": {
        "@builder.io/qwik": "0.0.20",
        "@builder.io/qwik-dom": "^2.1.18",
        "@fastify/cookie": "^6.0.0",
        "@fastify/session": "^8.2.0",
        "@fastify/static": "^5.0.2",
        "@ngneat/falso": "^5.4.0",
        "express": "4.17.3",
        "node-fetch": "^2.6.7",
        "fastify": "^3.29.0",
        "focus-trap": "^6.9.2",
        "focus-visible": "^5.2.0",
        "pino-pretty": "^7.6.1",
        "sass": "^1.51.0"
    }
}
